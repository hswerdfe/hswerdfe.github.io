all_vecs_just_one <- function(vec) {
vec |> map(length) |> unlist() |> max() == 1
}
un_list <- function(vec, null_replace = ""){
vec[sapply(vec, is.null)] <- null_replace
ret_val <- vec |> unlist()
if (length(vec) == length(ret_val)){
return(ret_val)
}else{
return(vec)
}
}
